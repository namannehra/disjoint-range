{
    "compilerOptions": {
        "allowSyntheticDefaultImports": false,
        "declaration": true,
        "lib": ["es2018"],
        "module": "es6",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "build",
        "strict": true,
        "target": "es2018"
    }
}